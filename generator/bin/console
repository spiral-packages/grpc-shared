#!/usr/bin/env php
<?php

require __DIR__ . '/../../vendor/autoload.php';

use Generator\Console\GeneratorCommand;
use Symfony\Component\Console\Application;

$application = new Application();

$application->add(
    new GeneratorCommand(
        files: new \Spiral\Files\Files(),
        rootDir: __DIR__ . '/../../',
    ),
);

$application->run();
